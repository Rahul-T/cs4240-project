main let
	type TEST = float;
	type TESTARR = array[10] of float;
	var A,B,D : float;
	var C: array[10] of int;
	var E, G: TEST;
	var F: int;

	function printf (n : float, f : int) :float begin
		B := n + 5.0;
		return B;
	end;

in begin
	for F := 1 to 2 do 
		A := 1.5;
	enddo;

	F := C[10] - F ** C[10];
	F := printi(F);

	/* hasdfasdfi */

	let
		var X: int;
	in
		X := 2;
	end;
	/* X := 3; asdfsdf _+@#$!@#)*$#@!$DSAF~@!#*/
	/* wow look
	a multline comment */

	A := 10.0;
end
